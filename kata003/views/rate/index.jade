    script(src='/vendor/angular-i18n/angular-locale_lt.js')
    script(src='/vendor/angular-route/angular-route.js')
    script(src='/vendor/angular-resource/angular-resource.js')
    script(src='/vendor/angular-cookies/angular-cookies.js')
    script(src='/vendor/angular-bootstrap/ui-bootstrap.js')
    script(src='/vendor/angular-bootstrap/ui-bootstrap-tpls.js')
    script(src='/vendor/fingerprint/fingerprint.js')

    script(src='/rate/app.js')
    script(src='/rate/resources/rate-resource.js')
    script(src='/rate/resources/comment-rating-resource.js')
    script(src='/rate/controllers/rate-controller.js')
    // scripts end

