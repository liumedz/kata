    link(rel='stylesheet', href='/rate/css/bootstrap.css')
    link(rel='stylesheet', href='/rate/css/style.css')
    link(rel='stylesheet', href='/vendor/jqplot/jquery.jqplot.css')
    // styles end
block content
    div(data-ng-app='app')
        include header
        .container
            div(data-ng-view)
        include content
